{
  "manifest_version": 2,
  "name": "noteme",
  "description": "noteme",
  "version": "1.0",
  "background": {
    "scripts": ["js/index.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}
